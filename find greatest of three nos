#include <iostream>
using namespace std;

int main() {
    
    int num1,num2,num3;
    
    cout << "Enter the value of num1 : ";
    cin>> num1;
    
    cout <<"Enter the value of num2 : ";
    cin>> num2;
    
    cout <<"Enter the value of num3 : ";
    cin>> num3;
    
    if(num1 > num2 && num1 > num3){
        cout<< num1 << " is greater";
    }else if (num2 > num1 && num2 > num3){
        cout<< num2 << " is greater";
    }else{
        cout<< num3 << " is greater";
    }

    return 0;
}

//Output
Enter the value of num1 : 45
Enter the value of num2 : 95
Enter the value of num3 : 63
95 is greater

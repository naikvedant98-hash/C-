#include <iostream>
using namespace std;

int main() {
    
    int num , value = 0;
		
	cout<<"Enter the value for num : ";
	cin>>num;
		
	int num1 = num;
		
	while(num1 > 0) {
		int rem = num1 % 10;
		value = value *10 + rem;
		num1 /= 10;
	}
		
	cout<<"Reversed elements are : "<<value;
	cin>>num;

    return 0;
}

//Output
Enter the value for num : 123
Reversed elements are : 321
